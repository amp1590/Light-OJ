<script>
    
    
    
    $(document).ready(function(){
    	
        
        var body = $('body'),
            $videoTriggers = $('.videoTrigger');

        if(window.limz.responsive.getResponsiveId() === 'mob'){
            $videoTriggers.each(function(){
                var $button = $(this),
                    $video = $button.parents('.videoDevice').find('video'),
                    $container = $button.parents('.videoDevice'),
                    video = $video[0];

                $container.on('click', function(){
                    if(video.paused){
                        video.play();
                    } else {
                        video.pause();
                    }

                });

            })
        }

        $videoTriggers.on('click', function(event){

            event.preventDefault();
            event.stopPropagation();

            var $button = $(this),
                $video = $button.parents('.videoDevice').find('video'),
                $container = $button.parents('.videoDevice'),
                video = $video[0];

            $button.hide();

            video.play();

            video.addEventListener('ended', function(){
                $button.show();
            }, false);

            $container.on('click', function(){
                video.pause();
                $button.show();
            });

        });

        function setInfoBoxListener(){
            body.on('mouseenter.limzInfoBox', '.limz_infoBox_trigger', function(){
                var $trigger = $(this),
                    id = $trigger.attr('data-info_box-id'),
                    $infoBox =  $('.limz_infoBox[data-info_box-id='+id+']'),
                    $content = $infoBox.find('.limz_infoBox_content').clone(),
                    bodyWidth = body.outerWidth();

                $trigger.append($content);



                if(bodyWidth < 400){
                    var triggerOffset = $trigger.offset().left,
                        regularLeft = (bodyWidth - $content.outerWidth()) / 2,
                        left = regularLeft - triggerOffset + 'px';

                    $content.css('left', left);

                } else {
                    if($content.offset().left + $content.outerWidth() > bodyWidth){
                        $content.css('right', '0');
                    } else {
                        $content.css('left', '0');
                    }
                }

                body.off('mouseenter.limzInfoBox');
                $content.on('mouseleave', function(){
                    $content.remove();
                    setInfoBoxListener();
                });

            });
        }

        setInfoBoxListener();


        jQuery('.fancybox').on('mousedown', function() {
            jQuery(this).fancybox({
                padding     : [50,50,50,50],
                maxWidth	: 800,
                maxHeight	: 600,
                fitToView	: true
            });
            return false;
        });

        jQuery('.fancybox').click(function() {
            return false;
        });



